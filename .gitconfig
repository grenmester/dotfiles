[user]
	name = Jacky Lee
	email = dr.jackylee@yahoo.com
[alias]
    pl = pull
    ci = commit
    ps = push
    cl = clone
    hist = log --graph --pretty=format:'%Cred%h%Creset %s%C(yellow)%d%Creset %Cgreen(%cr)%Creset [%an]' --abbrev-commit --date=relative
[color]
    ui = true
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = green
    changed = yellow
    untracked = red
[core]
    editor = /usr/local/bin/vim
[push]
    default = simple
[branch "master"]
    remote = origin
