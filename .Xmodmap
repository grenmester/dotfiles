! Alias Caps_Lock to Control_L
clear lock
clear control
keycode 66 = Control_L
add control = Control_L Control_R

! Swap Alt_L and Super_L
clear mod1
clear mod4
keycode 64 = Super_L
keycode 133 = Alt_L
add mod1 = Alt_L Alt_R
add mod4 = Super_L Super_R

! Alias Alt_R to Multi_key
keycode 108 = Multi_key
